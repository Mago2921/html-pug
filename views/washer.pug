  link(href='https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/css/bootstrap.min.css' rel='stylesheet' integrity='sha384-+0n0xVW2eSR5OomGNYDnhzAbDsOXxcvSN1TPprVMTNDbiYZCxYbOOl7+AMvyTG2x' crossorigin='anonymous')
head
  meta(charset='UTF-8')
  meta(name='viewport' content='width=device-width, initial-scale=1.0')
  meta(http-equiv='X-UA-Compatible' content='ie=edge')
  title ZaWash
  link(href='https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/css/bootstrap.min.css' rel='stylesheet' integrity='sha384-+0n0xVW2eSR5OomGNYDnhzAbDsOXxcvSN1TPprVMTNDbiYZCxYbOOl7+AMvyTG2x' crossorigin='anonymous')
  link(rel='stylesheet' href='projects/ZaWash/dist/css/bootstrap.min.css')
  link(rel='stylesheet' href='projects/ZaWash/dist/css/bootstrap.min.css.map')
  link(rel='stylesheet' href='D:\Projects\car wash\carwash.css' type='text/css')
.navbar
  .logolist
    ul
      li Home
      li About Us
      li Service
      li Register
.container.py-3
  header
    .d-flex.flex-wrap.align-items-center.justify-content-center.justify-content-md-between.py-3.mb-4.border-bottom
      a.d-flex.align-items-center.col-md-3.mb-2.mb-md-0.text-dark.text-decoration-none(href='index.html')
        svg.bi.me-2(width='40' height='32' role='img' aria-label='Bootstrap')
          use(xlink:href='#bootstrap')
        span.fs-4 ZaWash
  main
    .py-5.text-center
      img.d-block.mx-auto.mb-4(src='' alt='' width='72' height='57')
      h2 Register Here
    .col-md-7.col-lg-8
      h4 Car Registeration
      form.needs-validation(novalidate='')
        .row.g-3
          .col-sm-6
            label.form-label(for='Car Type') Car Type
            input#carType.form-control(type='text' placeholder='Range Rover' value='' required='')
          .mb-3.col-sm-6
            label.form-label(for='Number') Number Plate
            input#numberPlate.form-control(type='text' placeholder='UBA 123z' value='' required='')
          .col-sm-6
            label.form-label(for='Car color') Car color
            input#carColor.form-control(type='text' placeholder='green' value='' required='')
          .col-sm-6
            label.form-label(for='Car Make') Car Make
            input#carMake.form-control(type='text' placeholder='toyota' value='' required='')
      p
      h4 Check In Time
      p
      form.needs-validation(novalidate='')
        .row.g-3
          .col-sm-6
            label.form-label(for='Date') Date
            input#date.form-control(type='date' placeholder='green' value='' required='')
          .col-sm-6
            label.form-label(for='car arrival') Time of Arrival
            input#carArrival.form-control(type='time' placeholder='00:00 am' value='' required='')
          .mb-3.col-sm-6
            label.form-label(for='car departure') Time of Departure
            input#numberPlate.form-control(type='time' placeholder='00:00 pm' value='' required='')
      p
      h4 Payment
      p
      .my-3
        .form-check
          input#credit.form-check-input(name='paymentMethod' type='radio' checked='' required='')
          label.form-check-label(for='credit') Cash
        .form-check
          input#credit.form-check-input(name='paymentMethod' type='radio' checked='' required='')
          label.form-check-label(for='credit') MTN Momo 
        .form-check
          input#credit.form-check-input(name='paymentMethod' type='radio' checked='' required='')
          label.form-check-label(for='credit') Airtel Money
        .form-check
          input#credit.form-check-input(name='paymentMethod' type='radio' checked='' required='')
          label.form-check-label(for='credit') Other
      hr.my-4
      p
      h4 For Official Use Only
      p
      form.needs-validation(novalidate='')
        fieldset
          .mb-3.form-check
            label.form-label(for='car wash package') CarWash Package 
            select#state.form-select(required='')
              option(value='') Full Wash
              option Small Car
              option Medium
              option Full Wash 
              option Boda Boda
              option engine
              option Other
          .mb-3.form-check
            label.form-label(for='car washer') Car Washer
            input#kanaabe.form-control(type='text' placeholder='senfuka robert' value='' required='')
        hr.my-4
        button.w-100.btn.btn-primary.btn-lg(type='submit') Submit
footer.footer.mt-auto.py-3.bg-light
  .container
    span.text-muted &copy; 2021 Zaabu Washing Limited.
script.
